    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-spies": "^0.7.1",
    "enzyme": "^2.4.1",
    "enzyme-adapter-react-15.4": "^1.0.4",
    "file-loader": "^1.1.11",
    "husky": "^0.13.4",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "lodash": "^4.17.4",
    "match-screenshot": "^1.0.14",
    "nock": "^9.3.3",
    "node-sass": "^4.9.0",
    "puppeteer": "^1.5.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "sinon": "^6.0.0",
    "svg-inline-loader": "^0.8.0",
    "wallaby-webpack": "^3.9.7",
    "yoshi": "^1.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-runtime",
      "syntax-dynamic-import",
      "dynamic-import-webpack"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "development": {
        "presets": [
          "env"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "wix/esnext",
    "rules": {
      "no-unused-vars": 0,
      "camelcase": 0,
      "no-mixed-operators": 0,
      "new-cap": 0,
      "default-case": 0,
      "max-params": 0,
      "no-cond-assign": 0,
      "max-depth": 0,
      "no-useless-escape": 0,
      "no-eval": 0,
      "curly": 0,
      "curly-style": 0
    },
    "globals": {
      "fetch": true,
      "window": true,
      "document": true,
      "Image": true,
      "prerenderedGallery": true,
      "Event": true,
      "HTMLElement": true
    }
  },
  "unpkg": true,
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "yoshi": {
    "entry": {
      "main": "./index"
    },
    "externals": {
      "axios": "axios",
      "babel-runtime": "babel-runtime",
      "lodash": "_",
      "react": "React",
      "react-dom": "ReactDOM"
    },

    "exports": "pro-gallery-renderer"
  }
}
