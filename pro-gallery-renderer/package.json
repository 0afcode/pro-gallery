{
  "private": false,
  "name": "pro-gallery-renderer",
  "version": "1.0.0",
  "author": {
    "name": "Guy Sopher",
    "email": "guyso@wix.com",
    "url": ""
  },
  "main": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "yoshi start",
    "build": ":",
    "precommit": " ",
    "pretest": "yoshi build",
    "test": "yoshi test",
    "posttest": "yoshi lint",
    "release": "yoshi release"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "classnames": "^2.2.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "photography-client-lib": "^1.0.33",
    "pro-gallery-layouter": "^1.0.11",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-player-fork": "^0.12.106",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "wix-bi-logger": "1.0.485"
  },
  "devDependencies": {
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.5.0",
    "chai": "^4.1.2",
    "chai-spies": "^0.7.1",
    "enzyme": "^2.4.1",
    "enzyme-adapter-react-15.4": "^1.0.4",
    "husky": "^0.13.4",
    "jsdom": "11.2.0",
    "jsdom-global": "3.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.1",
    "yoshi": "latest"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      "syntax-dynamic-import",
      "dynamic-import-webpack"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "development": {
        "presets": [
          "env"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "wix/esnext",
    "rules": {
      "no-unused-vars": 0,
      "camelcase": 0,
      "no-mixed-operators": 0,
      "new-cap": 0,
      "default-case": 0,
      "max-params": 0,
      "no-cond-assign": 0,
      "max-depth": 0,
      "no-useless-escape": 0,
      "no-eval": 0
    },
    "globals": {
      "window": true,
      "document": true,
      "jquery": "$",
      "lodash": "_",
      "Image": true,
      "prerenderedGallery": true
    }
  },
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
