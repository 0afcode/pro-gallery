    "pro-gallery-layouter": "^2.0.16",
    "react-player-fork": "^0.12.106",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "wix-bi-logger": "^1.0.496"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "axios": "^0.16.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-links": "^3.4.7",
    "@storybook/addons": "^3.4.7",
    "@storybook/react": "^3.4.7",
    "axios": "^0.16.2",
    "babel-core": "^6.26.3",
